	Journal = {\apj},
	Keywords = {ASTRONOMICAL MODELS, DENSITY (MASS/VOLUME), ELLIPTICAL GALAXIES, GALACTIC CLUSTERS, GALACTIC MASS, GRAVITATIONAL LENSES, CURVES (GEOMETRY), IMAGES, RADII, VELOCITY},
	Month = jan,
