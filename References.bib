	Date-Modified = {2018-02-02 06:18:11 +0000},
	Doi = {10.1093/mnras/stu1931},
	Eprint = {1311.1614},
	Journal = {\mnras},
	Keywords = {gravitational lensing: strong, gravitational lensing: weak, galaxies: clusters: general, large-scale structure of Universe},
	Month = dec,
	Pages = {3581-3591},
	Title = {{The effect of large-scale structure on the magnification of high-redshift sources by cluster lenses}},
