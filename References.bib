	Eprint = {astro-ph/0004092},
	Journal = {\aj},
	Keywords = {Galaxies: Clusters: General, Methods: Data Analysis},
	Month = oct,
	Pages = {2148-2162},
	Title = {{A New Method For Galaxy Cluster Detection. I. The Algorithm}},
	Volume = 120,
	Year = 2000,
