	Eprint = {1710.07301},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Astrophysics of Galaxies},
	Month = oct,
	Title = {{Size--luminosity relations and UV luminosity functions at $z=6-9$ simultaneously derived from the complete Hubble Frontier Fields data}},
