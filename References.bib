	Author = {{Ishigaki}, M. and {Kawamata}, R. and {Ouchi}, M. and {Oguri}, M. and {Shimasaku}, K. and {Ono}, Y.},
	Date-Added = {2018-02-02 04:20:30 +0000},
	Date-Modified = {2018-02-02 04:20:31 +0000},
	Eprint = {1702.04867},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Astrophysics of Galaxies},
