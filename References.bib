	Date-Modified = {2014-10-09 15:46:57 +0000},
	Doi = {10.1007/s11214-013-9981-x},
	Eprint = {1303.3363},
	Journal = {\ssr},
	Keywords = {Cosmology, Galaxy clusters, Gravitational lensing},
	Month = aug,
	Pages = {31-74},
	Primaryclass = {astro-ph.CO},
