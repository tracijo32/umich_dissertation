	Author = {{El{\'{\i}}asd{\'o}ttir}, {\'A}. and {Limousin}, M. and {Richard}, J. and {Hjorth}, J. and {Kneib}, J.-P. and {Natarajan}, P. and {Pedersen}, K. and {Jullo}, E. and {Paraficz}, D.},
	Date-Added = {2014-04-27 02:15:55 +0000},
