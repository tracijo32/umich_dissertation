	Author = {{Limousin}, M. and {Richard}, J. and {Kneib}, J.-P. and {Brink}, H. and {Pell{\'o}}, R. and {Jullo}, E. and {Tu}, H. and {Sommer-Larsen}, J. and {Egami}, E. and {Micha{\l}owski}, M.~J. and {Cabanac}, R. and {Stark}, D.~P.},
