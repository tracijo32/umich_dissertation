	Date-Modified = {2014-01-09 20:39:44 +0000},
	Doi = {10.1051/0004-6361:20042233},
	Journal = {\aap},
	Keywords = {cosmology: dark, matter, galaxies: clusters: general, gravitational lensing},
	Month = jul,
	Pages = {39-48},
	Title = {{Strong and weak lensing united}},
	Volume = 437,
	Year = 2005,
	Bdsk-File-1 = {},
