	Author = {{Orban de Xivry}, G. and {Marshall}, P.},
	Date-Added = {2013-12-13 16:29:59 +0000},
	Date-Modified = {2013-12-13 16:30:00 +0000},
	Doi = {10.1111/j.1365-2966.2009.14925.x},
	Eprint = {0904.1454},
	Journal = {\mnras},
	Keywords = {gravitational lensing , surveys},
	Month = oct,
	Pages = {2-20},
	Primaryclass = {astro-ph.CO},
	Title = {{An atlas of predicted exotic gravitational lenses}},
	Volume = 399,
	Year = 2009,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1111/j.1365-2966.2009.14925.x},
