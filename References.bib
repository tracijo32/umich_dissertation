	Archiveprefix = {arXiv},
	Arxivurl = {http://arxiv.org/abs/1710.07301},
	Author = {{Kawamata}, R. and {Ishigaki}, M. and {Shimasaku}, K. and {Oguri}, M. and {Ouchi}, M. and {Tanigawa}, S.},
	Date-Added = {2018-02-02 04:25:11 +0000},
