	Author = {{K{\"o}hlinger}, F. and {Schmidt}, R.~W.},
	Date-Added = {2013-11-22 04:39:30 +0000},
	Date-Modified = {2013-11-22 04:39:31 +0000},
	Eprint = {1310.0021},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Cosmology and Extragalactic Astrophysics},
	Month = sep,
	Primaryclass = {astro-ph.CO},
	Title = {{Strong lensing in RX J1347.5-1145 revisited}},
	Year = 2013,
