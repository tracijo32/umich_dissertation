	Journal = {\apjl},
	Keywords = {dark matter, galaxies: abundances, galaxies: evolution},
	Month = jan,
	Pages = {L31},
	Primaryclass = {astro-ph.CO},
	Title = {{On the Lack of Evolution in Galaxy Star Formation Efficiency}},
